{
  "name": "digit-eci-reference-gatsby",
  "description": "The Gatsby.js client-side using digit-eci-reference Drupal 8 backend.",
  "version": "0.1.0",
  "author": "European Commission",
  "license": "EUPL-1.1",
  "private": true,
  "scripts": {
    "build:prefixed": "gatsby build --prefix-paths",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"{gatsby-*.js,src/**/*.js}\"",
    "lint-staged": "lint-staged",
    "lint:js": "eslint .",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test:lint": "npm run lint:js",
    "test": "npm-run-all -p test:* -cn"
  },
  "dependencies": {
    "@ecl/ec-preset-website": "2.5.0",
    "gatsby": "2.8.4",
    "gatsby-link": "2.1.1",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-source-drupal": "3.1.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-compat": "3.1.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "husky": "2.4.0",
    "lerna": "3.14.1",
    "lint-staged": "8.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.0",
    "prop-types": "15.7.2"
  },
  "workspaces": {
    "packages": [
      "packages/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ec-inno/digit-eci-reference"
  },
  "bugs": {
    "url": "https://github.com/ec-inno/digit-eci-reference"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
